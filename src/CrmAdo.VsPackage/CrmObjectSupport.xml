              <Parameter>
                <Parameter>
                  <Parameter value="{2}" />
                  <Parameter />
                  <Parameter value="Field" />
                  <Parameter />
                  <Parameter>
                    
        <!-- GUID_DSRefProperty_PreciseType -->
        
                    <Parameter value="39A5A7E7-513F-44a4-B79D-7652CD8962D9">
                      <Parameter value="102" type="System.Int32" />
                      
        <!-- Table_Column -->
        
                    </Parameter>
