  <Target Name="AfterBuild" DependsOnTargets="ILMerge" />
  <PropertyGroup>
    <ILMergeExecutable>"$(ProjectDir)..\..\lib\ILMerge.Tools.2.14.1208\tools\ILMerge.exe"</ILMergeExecutable>
    <SnExecutable>"$(ProjectDir)..\..\lib\sn\sn.exe"</SnExecutable>
    <!--<SnExecutable>"G:\Program Files\Microsoft SDKs\Windows\v7.0\Bin\sn.exe"</SnExecutable>-->
    <PublicKeyFile>"$(ProjectDir)Key.snk"</PublicKeyFile>
    <PfxKeyContainerName>VS_KEY_ABAB1234ABAB1234</PfxKeyContainerName>
    <PasswordProtectedeyFile>"$(ProjectDir)Key.pfx"</PasswordProtectedeyFile>
    <!--<OutputMergedFileDir>$(ProjectDir)$(OutputPath)merged\</OutputMergedFileDir>-->
  </PropertyGroup>
  <!--<Target Name="CreateMergedOutputDir">
    -->
  <!--Ensure a folder is created for the output of ILMerge-->
  <!--
    <RemoveDir ContinueOnError="true" Directories="$(OutputMergedFileDir)"/>
    <MakeDir Directories="$(OutputMergedFileDir)"/>
  </Target>-->
  <Target Name="ILMerge" Inputs="@(IntermediateAssembly)" Outputs="@(MainAssembly -> '%(RelativeDir)%(Filename).ILMergeTrigger%(Extension)')">
    <!--Merge any dependencies with an ILMerge property = true into this assembly.-->
    <CreateItem Include="@(ReferencePath)" Condition="'%(ReferencePath.ILMerge)'=='True'">
      <Output TaskParameter="Include" ItemName="ILMergeAssemblies" />
    </CreateItem>
    <Exec Command="$(ILMergeExecutable) /Closed /Internalize /Lib:&quot;$(ProjectDir)$(OutputPath.TrimEnd('\'))&quot; /keyfile:$(PublicKeyFile) /delaysign /out:&quot;$(ProjectDir)@(MainAssembly)&quot; &quot;$(ProjectDir)@(IntermediateAssembly)&quot; @(ILMergeAssemblies->'&quot;%(FullPath)&quot;', ' ')" />
    <!-- Make a copy of the merged output DLL to use as a trigger for incremental builds -->
    <Copy SourceFiles="@(MainAssembly)" DestinationFiles="@(MainAssembly -> '%(RelativeDir)%(Filename).ILMergeTrigger%(Extension)')" />
    <!--Re-sign the assemly using pfx-->
    <Exec Command="$(SnExecutable) -Rca &quot;$(ProjectDir)@(MainAssembly)&quot; $(PfxKeyContainerName)" />
  </Target>
